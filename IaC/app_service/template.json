                "hostNamesDisabled": false,
                "customDomainVerificationId": "787B78AE82F39FD773F1B15766B2F9F94A4B55F3A487B8C69718F5576EB10A9E",
                "containerSize": 0,
                "dailyMemoryTimeQuota": 0,
                "httpsOnly": false,
                "redundancyMode": "None",
                "storageAccountRequired": false,
                "keyVaultReferenceIdentity": "SystemAssigned"
            }
        },
